/* 轮播图 */
.index_swiper swiper{
    height: 340rpx;
}
.index_swiper image{
    width: 100%;
}

/* 导航 */
.index_cates{
    display: flex;
}
.index_cates navigator{
    flex: 1;
    padding: 20rpx;
}
.index_cates image{
    width: 100%;
}

/* 楼层 */
.floor_list{
    overflow: hidden;
    padding-bottom: 10rpx;
}
.floor_list navigator{
    float: left;
    width: 33.33%;
}
.floor_list navigator:nth-last-child(-n+4){
    height: 27.727112vw;
    padding-left: 10rpx;
}
.floor_list navigator:nth-last-child(-n+2){
    padding-top: 10rpx;
}
.floor_list image{
    width: 100%;
    height: 100%;
}